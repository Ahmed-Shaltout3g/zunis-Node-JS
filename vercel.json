{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
      
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],"functions": {
      "api/index.js": {
        "maxDuration": 30 
      },
      "api/*.js": {
        "maxDuration": 15 
      }
    }
    
  }